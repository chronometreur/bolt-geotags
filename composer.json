{
  "name": "chronolabs/geotags",
  "description": "Extension to add Geo tag into page",
  "type": "bolt-extension",
  "keywords": [
    "Analytics","SEO","Tools","Geotags"
  ],
  "require": {
    "bolt/bolt": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Victor Rodriguez",
      "email": "victor.rodriguez@chrono-labs.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Bolt\\Extension\\ChronoLabs\\Geotags\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bolt\\Extension\\ChronoLabs\\Geotags\\Tests\\": "tests",
      "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
    }
  },
  "extra": {
    "bolt-assets": "web",
    "bolt-class": "Bolt\\Extension\\ChronoLabs\\Geotags\\LeadInExtension"
  }
}
